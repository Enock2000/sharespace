# Lenco Payment API Configuration
# Copy this file to .env.local and fill in your values

# API Name
VSHR_API_NAME=VSHR

# Base URL for Lenco API
VSHR_BASE_URL=https://api.lenco.co/access/v2

# Lenco API Secret Key (Backend Only - DO NOT EXPOSE)
VSHR_SECRET_KEY=your_secret_key_here

# Lenco Public Key
VSHR_PUBLIC_KEY=your_public_key_here

# Frontend Public Key (exposed to browser)
NEXT_PUBLIC_VSHR_PUBLIC_KEY=your_public_key_here
